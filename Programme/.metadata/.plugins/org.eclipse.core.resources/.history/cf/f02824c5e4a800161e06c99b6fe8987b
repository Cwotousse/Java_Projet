package be.mousty.pojo;

public class ReservationCours {
	// VARIABLES
	private int numCours;
	private int numReservation;
	
	// CONSTRUCTEUR SANS ARGUMENTS
	public ReservationCours(){}
	
	// PROPRIETE
	public int getNumCours			() { return numCours; }
	public int getNumReservation	() { return numReservation; }
	public void setNumCours		(int el) 	{ this.numCours = el; }
	public void setNumReservation	(int el) 	{ this.numReservation = el; }
}
