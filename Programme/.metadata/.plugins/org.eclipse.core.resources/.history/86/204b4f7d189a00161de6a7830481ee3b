package POJO;

public class Accreditation {
	// VARIABLES
	private String nom;
	
	// CONSTRUCTEURS
	public Accreditation(){}
	public Accreditation(String nom){
		this.nom = nom;
	}
	
	// FONCTION SURCHARGEE
	@Override
	public String toString() { return nom; }
	
	// PROPRIETE
	public String getNom() { return nom; }
	public void setNom(String nom) { this.nom = nom; }
}
