package be.mousty.accessToDao;

import java.sql.Date;

public class CoursSemaineATD {
	// VARIABLES
	private Date dateDebutReserv;
	private Date dateFinReserv;


	// CONSTRUCTEUR SANS ARGUMENTS
	public CoursSemaineATD(){}
	public CoursSemaineATD(Date dateDebutReserv, Date dateFinReserv){
		this.dateDebutReserv 	= dateDebutReserv;
		this.dateFinReserv 		= dateFinReserv;
	}
	
	// METHODES
	

	// PROPRIETE
	public Date getDateDebutReserv	()	 					{ return dateDebutReserv; 					}
	public Date getDateFinReserv	() 						{ return dateFinReserv; 					}
	public void setDateDebutReserv	(Date dateDebutReserv) 	{ this.dateDebutReserv = dateDebutReserv; 	}
	public void setDateFinReserv	(Date dateFinReserv) 	{ this.dateFinReserv = dateFinReserv; 		}
}
