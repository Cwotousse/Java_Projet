package be.mousty.pojo;

public class ReservationEleve {
	// VARIABLES
	private int numEleve;
	private int numReservation;
	
	// CONSTRUCTEURS
	public ReservationEleve(){}
	
	// PROPRIETE
	public int getNumEleve			() { return numEleve; }
	public int getNumReservation	() { return numReservation; }
	public void setNumEleve			(int el) 	{ this.numEleve = el; }
	public void setNumReservation	(int el) 	{ this.numReservation = el; }
}