package be.mousty.pojo;

public class CoursMoniteur {
	// VARIABLES
	private int numMoniteur;
	private int numCours;
	
	
	// CONSTRUCTEUR SANS ARGUMENTS
	public CoursMoniteur(){}
	
	// PROPRIETE
	public int getNumCours			() { return numCours; }
	public int getNumMoniteur	() { return numMoniteur; }
	public void setNumCours		(int el) 	{ this.numCours = el; }
	public void setNumMoniteur	(int el) 	{ this.numMoniteur = el; }
}
